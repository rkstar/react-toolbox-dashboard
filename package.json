  "scripts": {
    "build:dev:server": "npm run clean:server && webpack --watch --config config/server/webpack.dev.js",
    "build:prod:server": "npm run clean:server && webpack --config config/server/webpack.prod.js",
    "build:dev:client": "webpack --config config/client/webpack.dev.js",
    "build:prod:client": "npm run clean:client && webpack --config config/client/webpack.prod.js",
    "clean:server": "rimraf dist/server/server.js* dist/server/*hot-update*",
    "clean:client": "rimraf dist/client/bundle.js*",
    "dev:server": "nodemon dist/server/server.js",
    "start": "npm run build:dev:client && npm run build:dev:server & npm run dev:server",
    "build": "npm run build:prod:client && npm run build:prod:server",
    "toolbox": "react-toolbox-themr --config config/client/react-toolbox-themr.json",
    "test": "echo \"Error: no test specified\" && exit 1"
